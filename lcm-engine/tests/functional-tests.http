### Get LCM Engine's health status
GET {{lcm_host}}/health


### Get the authentication status
GET {{lcm_host}}/auth/status


### List available workspaces
GET {{lcm_host}}/workspace


### Create a new TOSCA workspace
POST {{lcm_host}}/workspace

{
    "name": "TOSCA workspace"
}


### Create a new TOSCA project that requires no secrets
POST {{lcm_host}}/workspace/1/project

{
    "name": "hello-world",
    "kind": "si.xlab.lcm-service.tosca",
    "csar": ""
}


### Delete the TOSCA project
DELETE {{lcm_host}}/workspace/1/project/1


### List available secrets
GET {{lcm_host}}/secret


### Create a new secret containing environment variables and file
POST {{lcm_host}}/secret

{
    "name": "aws",
    "file": {
        "path": "/a/path",
        "contents": "aGVsbG8="
    },
    "env": {
        "AWS_ID": "my aws id",
        "AWS_KEY": "my aws key"
    }
}


### Get a specific secret
GET {{lcm_host}}/secret/1


### List secrets assigned to the workspace
GET {{lcm_host}}/workspace/1/secret


### Associate a secret with a workspace
PUT {{lcm_host}}/workspace/1/secret/1


### Create a new TOSCA project with all the workspace secrets injected
POST {{lcm_host}}/workspace/1/project
Content-Type: application/json

{
    "name": "aws-thumbnail-generator",
    "kind": "si.xlab.lcm-service.tosca",
    "csar": ""
}


### Delete the TOSCA project
DELETE {{lcm_host}}/workspace/1/project/2


### Remove the TOSCA secret from the workspace
DELETE {{lcm_host}}/workspace/1/secret/1


### Delete the TOSCA secret
DELETE {{lcm_host}}/secret/1


### Delete the TOSCA workspace
DELETE {{lcm_host}}/workspace/1


### Create a new Terraform workspace
POST {{lcm_host}}/workspace

{
    "name": "Terraform workspace"
}


### Create a secret for the Terraform workspace
POST {{lcm_host}}/secret

{
    "name": "Terraform IONOS credentials",
    "file": {
        "path": "/terraform-api/cred.tf",
        "contents":         "cHJvdmlkZXIgImlvbm9zY2xvdWQiIHsKICB1c2VybmFtZSAgICAgICAgICA9ICJzb21lLXVzZXJAaW9ub3MuY29tIgogIHBhc3N3b3JkICAgICAgICAgID0gIkg0cmQyYnIzNGshIgp9"
    }
}


### Associate the Terraform secret with the Terraform workspace
PUT {{lcm_host}}/workspace/2/secret/2


### List secrets associated with the Terraform workspace
GET {{lcm_host}}/workspace/2/secret


### Create a new Terraform project
POST {{lcm_host}}/workspace/2/project

{
    "name": "ionos-terraform",
    "kind": "si.xlab.lcm-service.terraform",
    "csar": ""
}


### Delete the Terraform project
DELETE {{lcm_host}}/workspace/2/project/3


### Remove the Terraform secrets from the workspace
DELETE {{lcm_host}}/workspace/2/secret/2


### Delete the Terraform secret
DELETE {{lcm_host}}/secret/2
